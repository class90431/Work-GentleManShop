  console.log(response.data)\n        vm.tempProducts = response.data.products\n        vm.isLoading = false\n        console.log(vm.tempProducts)\n        vm.pagination = response.data.pagination\n        // console.log(vm.pagination)\n        vm.products = vm.tempProducts.filter(function (item, key, arr) {\n          return item.category === '熱賣商品'\n        })\n        console.log(vm.products)\n      })\n    },\n    getOneProduct (item) {\n      const api = `${process.env.VUE_APP_API_PATH}/api/${process.env.VUE_APP_CUSTOMER_PATH}/product/${item.id}`\n      // const vm = this\n      // vm.isLoading = true\n      // this.$http.get(api).then((response) => {\n      //   console.log(response.data)\n      //   vm.products = response.data.products\n      //   vm.isLoading = false\n      //   console.log(vm.products)\n      //   vm.pagination = response.data.pagination\n      //   console.log(vm.pagination)\n      // })\n      console.log(api)\n    }\n  },\n  created () {\n    this.getProducts()\n  },\n  components: {\n    Pagination\n  }\n}\n\n</script>\n<style lang=\"scss\" scoped>\n  @import \"../assets/all.scss\";\n  .carousel .carousel-item img {\n    min-height: 300px;\n    max-height: 400px;\n    object-fit: cover;\n  }\n  .main {\n    margin-top: 150px;\n  }\n  .card-img-top {\n    height: 350px;\n    background-position: center;\n    background-size: cover\n  }\n  .text-decoration-through {\n    text-decoration:line-through;\n  }\n  .text-opacity50 {\n    opacity: 0.5;\n  }\n</style>\n"],"sourceRoot":"src/views"}]}