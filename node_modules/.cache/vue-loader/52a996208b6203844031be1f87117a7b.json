rcesContent":["<template>\n  <div class=\"cart\">\n    cart\n    <loading :active.sync=\"isLoading\"></loading>\n      <div class=\"main\">\n        <div class=\"container\">\n          <div class=\"row\">\n            <!-- 購物車表格 -->\n            <table class=\"table table-striped\">\n              <thead>\n                <tr>\n                  <th width=\"80\" scope=\"col\">#</th>\n                  <th scope=\"col\">品名</th>\n                  <th scope=\"col\">數量</th>\n                  <th width=\"120\" scope=\"col\">單價</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-for=\"(item, index) in carts\" :key=\"index\">\n                  <td>\n                    <button type=\"button\" class=\"btn btn-outline-danger btn-sm\">\n                      <i class=\"far fa-trash-alt\" ></i>\n                    </button>\n                  </td>\n                  <td>{{ item.product.title }}</td>\n                  <td>{{ item.qty }} / {{ item.product.unit }}</td>\n                  <td class=\"text-right\">{{ item.total }}</td>\n                </tr>\n                <tr class=\"bg-info text-white text-right\">\n                  <td></td>\n                  <td></td>\n                  <td >總價</td>\n                  <td><del class=\"text-white\">總價</del></td>\n                  <td v-if=\"carts.total == carts.finaltotal\" class=\"text-right\">{{ carts.total }}</td>\n                  <td v-else class=\"text-right\"><del class=\"text-white\">{{ carts.total }}</del></td>\n                </tr>\n                <tr class=\"text-right\" >\n                  <td></td>\n                  <td></td>\n                  <td class=\"text-success\"><strong>折扣價</strong></td>\n                  <td class=\"text-success text-right\"><strong>{{ carts.finaltotal }}</strong></td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n        <!-- Pagination -->\n        <Pagination :paginationComponent=\"pagination\" v-on:turnpage=\"getProducts\"></Pagination>\n      </div>\n  </div>\n</template>\n\n<script>\nimport Pagination from '../components/Pagination'\nexport default {\n  data () {\n    return {\n      products: [],\n      product: {},\n      isLoading: false,\n      pagination: {},\n      status: {\n        loadingItem: ''\n      },\n      carts: {},\n      coupon_code: '',\n      form: {\n        user: {\n          name: '',\n          email: '',\n          tel: '',\n          address: ''\n        },\n        message: ''\n      }\n    }\n  },\n  methods: {\n    getProducts (page = 1) { // 取得產品列表（不需驗證 API）\n      const api = `${process.env.VUE_APP_API_PATH}/api/${process.env.VUE_APP_CUSTOMER_PATH}/products?page=${page}`\n      const vm = this\n      vm.isLoading = true\n      this.$http.get(api).then((response) => {\n        console.log(response.data)\n        vm.products = response.data.products\n        vm.isLoading = false\n        console.log(vm.products)\n        vm.pagination = response.data.pagination\n        // console.log(vm.pagination)\n      })\n    },\n    getCart () {\n      const vm = this\n      const api = `${process.env.VUE_APP_API_PATH}/api/${process.env.VUE_APP_CUSTOMER_PATH}/cart`\n      vm.isLoading = true\n      this.$http.get(api).then((response) => {\n        console.log(response)\n        vm.isLoading = false\n        vm.carts = response.data.data.carts\n        vm.carts.total = response.data.data.total\n        vm.carts.finaltotal = response.data.data.final_total\n        console.log(vm.carts, vm.carts.total)\n      })\n    }\n  },\n  created () {\n    this.getProducts()\n    this.getCart()\n  },\n  components: {\n    Pagination\n  }\n}\n\n</script>\n<style lang=\"scss\" scoped>\n  @import \"../assets/all.scss\";\n  .carousel .carousel-item img {\n    min-height: 300px;\n    max-height: 400px;\n    object-fit: cover;\n  }\n  .main {\n    margin-top: 150px;\n  }\n  .card-img-top {\n    height: 350px;\n    background-position: center;\n    background-size: cover\n  }\n  .text-decoration-through {\n    text-decoration:line-through;\n  }\n  .text-opacity50 {\n    opacity: 0.5;\n  }\n</style>\n"]}]}